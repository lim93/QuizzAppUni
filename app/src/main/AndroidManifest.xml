<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quizzappuni.com.whs.quizzappuni.quizzappuni" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_launch"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity
            android:name="quizzappuni.com.whs.quizzappuni.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="quizzappuni.com.whs.quizzappuni.LearnMultiplechoice"
            android:label="@string/title_activity_learn_multiplechoice" >
        </activity>

        <activity
            android:name="quizzappuni.com.whs.quizzappuni.DB"
            android:label="@string/title_activity_db" >
        </activity>

    </application>

</manifest>
